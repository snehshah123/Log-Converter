    border-right: 6px solid;
    border-top: 6px solid rgb(255, 255, 255);
}

/* QCalendarWidget styles */
QCalendarWidget {
    border: 1px solid rgb(255, 255, 255);
    border-radius: 5px;
}

/* QCalendarWidget navigation bar */
QCalendarWidget QWidget#qt_calendar_navigationbar {
    background-color: #1E90FF; /* Background color of the navigation bar */
}

/* QCalendarWidget header row */
QCalendarWidget QWidget#qt_calendar_navigationbar QToolButton {
    color: white; /* Text color of the navigation bar buttons */
    background-color: #1E90FF; /* Background color of the navigation bar buttons */
    border: none; /* No border */
}

QCalendarWidget QToolButton#qt_calendar_prevmonth, 
QCalendarWidget QToolButton#qt_calendar_nextmonth {
    border: none;
    qproperty-icon: none;
    width: 20px;
    height: 20px;
}

QCalendarWidget QToolButton::menu-indicator {
    width: 0px;
}

/* QCalendarWidget header row - month and year */
QCalendarWidget QToolButton#qt_calendar_monthbutton, 
QCalendarWidget QToolButton#qt_calendar_yearbutton {
    color: white; /* Text color for month and year */
    background-color: black; /* Background color for month and year */
    border: none; /* No border */
}

/* Ensure the text color is white when these buttons are focused, active, or selected */
QCalendarWidget QToolButton#qt_calendar_monthbutton:pressed, 
QCalendarWidget QToolButton#qt_calendar_yearbutton:pressed, 
QCalendarWidget QToolButton#qt_calendar_monthbutton:focus, 
QCalendarWidget QToolButton#qt_calendar_yearbutton:focus, 
QCalendarWidget QToolButton#qt_calendar_monthbutton:checked, 
QCalendarWidget QToolButton#qt_calendar_yearbutton:checked {
    color: white; /* Text color for month and year when focused, pressed, or checked */
}

/* QMenu (dropdown list) styling for the month list */
QMenu {
    background-color: #1E90FF; /* Background color of the dropdown menu */
    color: white; /* Text color of the dropdown menu items */
    border: 1px solid white;
}

QMenu::item {
    background-color: black; /* Background color of each item */
    color: white; /* Text color of each item */
}

QMenu::item:selected {
    color: #1E90FF; /* Text color of the selected item */
}

/* QCalendarWidget selected date */
QCalendarWidget QAbstractItemView:enabled {
    color: blue; /* Text color */
    selection-background-color: #1E90FF; /* Background color for selected date */
    selection-color: black; /* Text color for selected date */
    border: none;
}

QCalendarWidget QAbstractItemView {
    outline: 0;
}
